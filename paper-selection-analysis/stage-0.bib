@ARTICLE{Upadhyay2022187,
author={Upadhyay, V. and Bhatnagar, T. and Holloway, C. and Rao, P.V.M. and Balakrishnan, M.},
title={Can Route Previews Amplify Building Orientation for People with Visual Impairment?},
