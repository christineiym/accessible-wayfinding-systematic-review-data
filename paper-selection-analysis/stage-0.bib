@CONFERENCE{Habha2021199,
author={Habha, L. and Trivedi, U. and Alqasemi, R. and Dubey, R.},
title={Autonomous Wheelchair Indoor-Outdoor Navigation System through Accessible Routes},
journal={ACM International Conference Proceeding Series},
year={2021},
pages={199-202},
doi={10.1145/3453892.3453997},
note={cited By 0; Conference of 14th ACM International Conference on PErvasive Technologies Related to Assistive Environments, PETRA 2021 ; Conference Date: 29 June 2021 Through 1 July 2021;  Conference Code:169934},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85109340508&doi=10.1145%2f3453892.3453997&partnerID=40&md5=59700eab717589633808d01ca1531fad},
affiliation={University of South Florida, United States},
abstract={Persons with disabilities who use power wheelchairs often have difficulty finding accessible routes to their destination from their outdoor location into the desired building or from their indoor location into another destination. This challenge can be alleviated through sensory information and preloaded map of the building and its surrounding outdoor areas. In this work, a power wheelchair system is integrated with a sensory suite and an autonomous control module to navigate the wheelchair in autonomous mode. A landmark-based autonomous navigation system using Quick Response (QR) code and Global Positioning System (GPS) technology is used for automatic communication between the power wheelchair and various locations in a designated building. For the outdoor navigation system, Autonomous Wheelchair Indoor-Outdoor Navigation System (AWI-ONS) relies on GPS signals and Google Maps. For indoor navigation, QR codes are placed in several key locations inside and outside the building, such as parking lots and garages, doorways, offices, bathrooms, stores, elevators, accessible entrances, and passageways. When scanned by the onboard camera, the AWI-ONS downloads the building floorplan from the Firebase server for indoor navigation. The floor plan with QR code information generates a topological map that is made available to the user through a touch screen user interface (UI). The user can select the destination, and AWI-ONS will generate the most viable and accessible path to the desired location using modified Breadth-First Search (BFS) algorithm. The AWI-ONS is fitted with Ultrasound-based obstacle avoidance system that is designed to avoid a possible collision while navigating towards the destination. © 2021 ACM.},
author_keywords={Autonomous navigation;  GPS;  Power wheelchair;  QR code},
keywords={Buildings;  Global positioning system;  Location;  Navigation;  Touch screens;  User interfaces;  Wheelchairs, Automatic communication;  Autonomous navigation systems;  Autonomous wheelchair;  Breadth first search algorithms;  Designated buildings;  Obstacle-avoidance system;  Outdoor navigation systems;  Persons with disabilities, Indoor positioning systems},
