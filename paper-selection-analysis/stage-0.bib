@ARTICLE{Khalili20212550,
author={Khalili, M. and Kryt, G. and Van Der Loos, H.F.M. and Borisoff, J.F.},
title={A Comparison between Conventional and Terrain-Specific Adaptive Pushrim-Activated Power-Assisted Wheelchairs},
journal={IEEE Transactions on Neural Systems and Rehabilitation Engineering},
year={2021},
volume={29},
