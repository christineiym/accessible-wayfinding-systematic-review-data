@CONFERENCE{Sharif20235,
author={Sharif, A. and Ramesh, A. and Yu, Q. and Nguyen, T.-A.H. and Xu, X.},
title={UnlockedMaps: A Web-Based Map for Visualizing the Real-Time Accessibility of Urban Rail Transit Stations},
journal={ACM International Conference Proceeding Series},
year={2023},
pages={5-17},
doi={10.1145/3587281.3587283},
note={cited By 0; Conference of 20th International Web for All Conference, W4A 2023 ; Conference Date: 30 April 2023 Through 1 May 2023;  Conference Code:188093},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85158159882&doi=10.1145%2f3587281.3587283&partnerID=40&md5=efa10a27c94fa9d1b1aa1fb18d81ae68},
affiliation={Paul G. Allen School of Computer Science & Engineering | DUB Group, University of Washington, Seattle, WA, United States; Applied and Computational Mathematical Sciences, University of Washington, Seattle, WA, United States; Department of Political Science, Duke University, Durham, NC, United States; Information School | DUB Group, University of Washington, Seattle, WA, United States},
abstract={Limited visibility into real-Time elevator statuses at transit stations in current web-based maps disenfranchises urban rail commuters who rely on functioning station elevators (e.g., wheelchair users). We shed light on the experiences of such commuters through interviews with representatives from five stakeholder groups (people with mobility disabilities, pregnant people, cyclists/stroller users/heavy-equipment carriers, disability advocates, and civic hackers). Motivated by our findings, we developed UnlockedMaps-an open-data map that visualizes the real-Time elevator status (accessible, experiencing at least one elevator outage, not-Accessible) of urban rail stations in six North American cities and displays nearby accessible restaurants and restrooms, assisting users in making informed decisions regarding their commute. We assessed UnlockedMaps by conducting (1) a user study, (2) controlled and "in-The-wild"longitudinal studies, and (3) follow-up interviews with our stakeholder groups. Our findings show that UnlockedMaps benefits all stakeholder groups, including those who do not rely on functioning elevators. © 2023 Owner/Author.},
