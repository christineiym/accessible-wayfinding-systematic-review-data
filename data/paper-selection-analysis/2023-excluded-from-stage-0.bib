@ARTICLE{Postorino2023,
author={Postorino, M.N. and Sarnè, G.M.L.},
title={Using Reputation Scores to Foster Car-Sharing Activities},
journal={Sustainability (Switzerland)},
year={2023},
volume={15},
number={4},
doi={10.3390/su15043295},
