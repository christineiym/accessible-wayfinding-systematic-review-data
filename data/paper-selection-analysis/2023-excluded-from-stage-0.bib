@ARTICLE{Meng2023,
author={Meng, C. and Jiang, S. and Wu, M. and Xiao, X. and Tao, D. and Gao, R.},
title={Smartphone-Based Indoor Floor Plan Construction via Acoustic Ranging and Inertial Tracking †},
journal={Machines},
year={2023},
