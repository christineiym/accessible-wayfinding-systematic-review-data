@ARTICLE{Ju2023,
author={Ju, Y. and Wang, Z. and Ju, X. and Cao, B. and Chen, C. and Lin, B.},
title={Understanding occupancy patterns of university libraries in the post-pandemic era},
journal={Energy and Buildings},
year={2023},
volume={291},
doi={10.1016/j.enbuild.2023.113138},
art_number={113138},
note={cited By 0},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85158885600&doi=10.1016%2fj.enbuild.2023.113138&partnerID=40&md5=7ff404657f989c8855b601506f02ab7f},
affiliation={Center for the Built Environment (CBE), University of California, Berkeley, CA  94720, United States; School of Architecture, Tsinghua University, Beijing, 100084, China; HKUST Shenzhen-Hong Kong Collaborative Innovation Research Institute, Shenzhen, Futian, China; Department of Civil and Environmental Engineering, The Hong Kong University of Science and Technology, Hong Kong},
abstract={Understanding occupancy patterns in buildings is crucial for energy system planning, energy model calibration, and occupant-centric operations for health and comfort. In this study, we detected and analyzed the occupancy patterns of university libraries, a building type that is largely overlooked in existing studies but can provide a unique opportunity to understand the human-building interactions given that occupants are free to choose their seats from hundreds of possible options in built environments. In libraries, occupants usually do not have fixed seats and pick seats within the built environment that they like, which provides a non-intrusive opportunity to carry out post-occupancy evaluations. We used the publicly available seat reservation data to analyze the occupancy patterns over one semester in four libraries, and proposed four metrics to capture important mobility parameters and behaviors at multiple spatial–temporal scales. We constructed two sets of databases consisting of around 70 million records of nearly 3,000 seats in 4 libraries, with seat-level resolution and sampling frequency of every 10 s. The informative data set depicts not only the overall spatial–temporal occupancy patterns, but also nuances associated with seats and visits. The daily flow of the main library exceeded two visits per seat. Half of the visitors stayed at the libraries for 3–6 h during a single occupancy. Semester schedules and campus accessibility together influence students’ decisions on when and to which library to go to, while even within the same zone, some seats were always more popular than their neighbors. We found that influential factors included privacy, distance to neighboring users, access to the outdoor environment (window), etc. besides the well-studied conventional Indoor Environment Quality factors whose importance might have been over-emphasized, and we summarize the balance of these attractive features as “semi-isolation”. This work provides fresh data for library occupancy modeling. Our findings can inform the design, operation and management of university libraries, emphasizing the importance of human-centric, sensory and biophilic designs. © 2023 Elsevier B.V.},
author_keywords={Human-centric design and operation;  Indoor environment quality (IEQ);  Occupancy patterns;  Post-occupancy evaluation (POE);  University library},
keywords={Quality control;  Structural design, Built environment;  Design and operations;  Human-centric design and operation;  Human-centric designs;  Indoor environment quality;  Indoor environments qualities;  Occupancy pattern;  Post occupancy evaluation;  Post-occupancy evaluation;  University libraries, Libraries},
funding_details={HZQB-KCZYB-2020083},
