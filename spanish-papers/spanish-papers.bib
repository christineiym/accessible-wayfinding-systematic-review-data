@CONFERENCE{FernandezGil2022,
author={Fernandez Gil, J.M. and Soriano Ubeda, S. and Molina Carmona, R.},
title={Unidigital Project: The Accessible University of the 21thCentury : Towards the digital transformation of the Spanish University System},
journal={CONTIE 2022 - 5th International Conference on Inclusive Technologies and Education and the 2nd International Conference of the Project Promoting Accessibility of Students with Disability to Higher Education in Cuba, Costa Rica and Dominican Republic},
year={2022},
doi={10.1109/CONTIE56301.2022.10004412},
note={cited By 0; Conference of 5th International Conference on Inclusive Technologies and Education, CONTIE 2022 ; Conference Date: 25 October 2022 Through 28 October 2022;  Conference Code:185881},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85146708474&doi=10.1109%2fCONTIE56301.2022.10004412&partnerID=40&md5=f937cedf6296f4e35b89158ab61bd3b8},
affiliation={Universidad de Alicante, Unidad de Accesibilidad Digital, Alicante, Spain; Universidad de Alicante, Vicerrectorado de Estudiantes y Empleabilidad, Alicante, Spain; Universidad de Alicante, Vicerrectorado de Transformación Digital, Alicante, Spain},
abstract={In January 2021 the Ministry of Universities supported by European funds, launched an ambitious project whose ultimate goal is to achieve the digitisation of the Spanish University System. This project is based in three main objectives: digital transformation, innovation and modernisation from a digital perspective. In this sense, the University of Alicante intends to develop a strategic line based on different support measures and grants for digitisation and teaching, focused particularly on students with disabilities. Likewise, it is meant to reduce the digital gap that exists today in our education system. For this reason, our university has been funded with a total of 50,000€ for the purchase of accessible technology. In higher education we are aware that we must address the ever-increasing demand for technological needs for the adequate training of our students. Currently, our university is equipped with 71 computer classrooms, but almost all of them lack a fully accessible workstation. Therefore, with the UNIDIGITAL project, we aim to provide all computer rooms with the suitable computer equipment and to adapt them to the special needs of our students (27-inch monitors, mouse, and keyboards with long cables to facilitate mobility, adapted tables, etc.). So far, this program has allowed us to purchase wireless microphones to be used with speech-to-text transcription systems (such as TAL or ListenALL) or to purchase analogue and digital systems for assisted listening. In addition, we have updated the software licenses for the Jaws screen reader and ZoomText magnifier. The implementation of this project will be carried out throughout the years 2022 and 2023. This funding plan aims to achieve a 10% increase in the digitisation rate of the Spanish university system by 2025, compared to 2019. © 2022 IEEE.},
author_keywords={digital gap;  Digitisation;  disability;  educational support},
keywords={Computer workstations;  E-learning;  Mammals;  Purchasing;  Sales, Computer classroom;  Digital gap;  Digital transformation;  Digitisation;  Disability;  Education systems;  Educational support;  High educations;  Support measure;  University system, Students},
funding_details={Ministerio de UniversidadesMinisterio de Universidades, MIU},
funding_text 1={Abstract — In January 2021 the Ministry of Universities supported by European funds, launched an ambitious project whose ultimate goal is to achieve the digitisation of the Spanish University System. This project is based in three main objectives: digital transformation, innovation and modernisation from a digital perspective. In this sense, the University of Alicante intends to develop a strategic line based on different support measures and grants for digitisation and teaching, focused particularly on students with disabilities. Likewise, it is meant to reduce the digital gap that exists today in our education system. For this reason, our university has been funded with a total of 50,000€ for the purchase of accessible technology. In higher education we are aware that we must address the ever-increasing demand for technological needs for the adequate training of our students. Currently, our university is equipped with 71 computer classrooms, but almost all of them lack a fully accessible workstation. Therefore, with the UNIDIGITAL project, we aim to provide all computer rooms with the suitable computer equipment and to adapt them to the special needs of our students (27-inch monitors, mouse, and keyboards with long cables to facilitate mobility, adapted tables, etc.). So far, this program has allowed us to purchase wireless microphones to be used with speech-to-text transcription systems (such as TAL or ListenALL) or to purchase analogue and digital systems for assisted listening. In addition, we have updated the software licenses for the Jaws screen reader and ZoomText magnifier. The implementation of this project will be carried out throughout the years 2022 and 2023. This funding plan aims to achieve a 10% increase in the digitisation rate of the Spanish university system by 2025, compared to 2019.},
references={Silió, E., (2021) Más gigas y ciencia abierta para acabar con la precariedad tecnológica de la Universidad, , El País Universidades, Febrero; https://tic.crue.org/publicaciones/universitic-2020/; Indice de digitalización universitaria, , https://www.universidades.gob.es/portal/site/universidades/menuitem.a9621cf716a24d251662c810026041a0/vgnextoid=af7506a6ff6ac710VgnVCM1000001d04140aRCRD; https://www.iste.org/es/standards/iste-standards-for-students; Plan Nacional de Competencias Digitales. Plan de Recuperación, Transformación y Resiliencia. Gobierno de Espana},
correspondence_address1={Fernandez Gil, J.M.; Universidad de Alicante, Spain; email: josema.fernandez@ua.es},
publisher={Institute of Electrical and Electronics Engineers Inc.},
